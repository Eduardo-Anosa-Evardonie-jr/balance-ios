target 'Balance' do
  platform :ios, '12.1'
  use_frameworks!

  # Pods for Balance
  pod 'SwiftEntryKit', '0.8.8'
  pod 'SnapKit', '~> 4.0.0'
  pod 'CryptoSwift'
  pod 'Intercom'
  pod 'PagingKit'
  pod 'web3.swift', :git => 'https://github.com/balance-io/web3.swift.git'

  # For PagingKit
  inherit! :search_paths
end
